          <td><%= score.round.date_played %></td>
          <td><%= score.hole_number %></td>
          <td><%= score.strokes %></td>
          <td><%= score.putts %></td>
        <tr>
    <% end %>
    </table>
    </div>
    <div class="span6">

    </div>
  </div>
</div>
